        "colab": {}
      },
      "source": [
        "s1 = statewise.melt(id_vars=['Date', 'Status']).sort_values(['Date','Status','variable'])"
      ],
      "execution_count": 0,
      "outputs": []
    },
    {
      "cell_type": "code",
